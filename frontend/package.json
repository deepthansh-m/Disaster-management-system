{
  "name": "disaster-prediction-system",
  "version": "1.0.0",
  "description": "A system for predicting and managing disasters",
  "main": "dist/index.js",
  "scripts": {
    "start": "parcel src/index.html --public-url /",
    "build": "parcel build src/index.html --public-url /",
    "build:css": "tailwindcss -i ./src/styles/main.css -o ./src/styles/output.css --watch",
    "dev": "concurrently \"npm run start\" \"npm run build:css",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "chart.js": "^4.4.7",
    "cors": "^2.8.5",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.469.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "buffer": "^6.0.3",
    "concurrently": "^9.1.2",
    "parcel": "^2.13.3",
    "postcss": "^8.4.49",
    "process": "^0.11.10",
    "tailwindcss": "^3.4.17"
  }
}
